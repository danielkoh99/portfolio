{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["plugin:react/recommended", "airbnb"],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "arrowFunctions":true
    },
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "plugins": ["react"],
  "rules": {
    "consistent-return":"off",
    "array-callback-return":"off",
    "no-unused-vars":"warn",
    "react/prop-types":"off"
  }
}
